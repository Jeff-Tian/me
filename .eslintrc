{
  "extends": [
    "taro",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "Taro"
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint",
  "plugins": [
    "typescript",
    "prettier"
  ]
}
